@using Tatil_Seyahat_Sitesi_Yeni.Models.Siniflar
@model List<Blog>

<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    @{int i = 0; }
        @foreach (var item in Model)
        {
        <ul class="list list-pad">
            <li class="list-bg">@{i++;
            } @i.  <a href="/Blog/BlogDetay/@item.Id">@item.Baslik </a> </li>
        </ul>
    }

</div>
